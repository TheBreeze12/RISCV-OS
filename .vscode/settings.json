  "C_Cpp.default.includePath": [
    "${workspaceFolder}/**",
    "${workspaceFolder}/include",
    "${workspaceFolder}/kernel/boot",
    "${workspaceFolder}/kernel",
    "${workspaceFolder}/kernel/mm"
  ],
  "C_Cpp.default.defines": [],
