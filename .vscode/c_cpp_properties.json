{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/kernel",
        "${workspaceFolder}/kernel/mm"
      ],
      "forcedInclude": [
        "${workspaceFolder}/kernel/type.h"
      ],
      "defines": [
        "__riscv",
        "__riscv_xlen=64"
      ],
      "compilerPath": "/usr/bin/riscv64-unknown-elf-gcc",
      "cStandard": "c11",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compileCommands": "${workspaceFolder}/compile_commands.json",
      "browse": {
        "path": [
          "${workspaceFolder}/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}